{
    "name": "mikedeveloper/symfony-helpers-bundle",
    "type": "symfony-bundle",
    "description": "Easy utilities for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Michelangelo Lacatena",
            "email": "michelangelo.lacatena@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "sensio/framework-extra-bundle": "^4.0 || ^5.0",
        "symfony/config": "^3.4.35 || ^4.4.7 || ^5.0.7",
        "symfony/console": "^3.4.35 || ^4.4.7 || ^5.0.7",
        "symfony/dependency-injection": "^3.4.35 || ^4.4.7 || ^5.0.7",
        "symfony/dotenv": "^4.4|^5.2",
        "symfony/yaml": "^4.4|^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.1",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Mikedevs\\HelpersBundle\\": "src"
        }
    }
}
